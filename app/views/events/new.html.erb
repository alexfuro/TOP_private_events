<%= form_for(@event) do |f| %>
  <%= render 'shared/errors', object: f.object %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :event_date %>
  <%= f.date_field :event_date %>


  <%= f.submit "Create Event" %>

<% end %>
